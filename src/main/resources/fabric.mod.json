{
  "schemaVersion": 1,
  "id": "cbclient",
  "version": "${version}",

  "name": "CB Client (Cave Brightness Mod)",
  "description": "A client mod that can be installed to Fabric that allows you to further modify your experience.",
  "authors": [
    "cchyphon"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/cb-client/",
    "sources": "https://github.com/DieselDorky16/cavebirghtnessmod",
    "issues": "https://github.com/DieselDorky16/cavebirghtnessmod/issues"
  },

  "license": "CC0-1.0",
  "icon": "assets/cbclient/icon.png",

  "environment": "client",
  "entrypoints": {
    "main": [
      "com.cchyphon.cavebrightnessmod.ModEntrypoint"
    ],
    "client": [
      "com.cchyphon.cavebrightnessmod.client.MainClientEntrypoint"
    ]
  },
  "mixins": [

  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17"
